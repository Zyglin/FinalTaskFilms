{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["standard", "standard-react","airbnb-base","prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react","prettier"],
  "rules": {
    "react/jsx-no-duplicate-props":0,
    "no-nested-ternary":0,
    "import/no-mutable-exports":0,
    "consistent-return":0,
    "class-methods-use-this": 0,
    "no-multi-assign":0,
    "no-unused-vars": ["warn", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "prettier/prettier": ["warn"]
  }
}